<div class="question_field">
  <%= f.label :title, "Question" %>
  <%= link_to "remove", '#', class: "remove_question" %><br>
  <%= f.text_area :title, rows: 1 %>
  <%= f.hidden_field :_destroy %>

<%= f.fields_for :answers do |builder| %>
  <%= render 'answer_fields', f: builder %>
<% end %>
</div>
